<%if(success && success.length){%>
<div class="alert alert-success alert-dissmissible fade show" role="alert" style="display: flex; justify-content: space-between;">
    <%= success %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>
<%if(error && error.length){%>
    <div class="alert alert-danger alert-dissmissible fade show" role="alert" style="display: flex; justify-content: space-between;">
        <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <% } %>